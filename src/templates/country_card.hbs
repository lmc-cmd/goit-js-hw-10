{{#each this}}
  <li class='gallery__item'> 
    <img
      src={{this.flags.png}} width="60" height="40"
      alt='flag of'{{this}}
      class='gallery__img'
    />
    <h2>{{this.name.official}} </h2>
    
    {{!-- <p>{{this.}} </p>  --}}
  </li>
  <p><b> Capital:</b> {{this.capital}} </p>
    <p><b> Population:</b> {{this.population}} </p>
    <p><b> Languages:</b>
    {{#each languages}}{{this}}{{#unless @last}},{{/unless}};
    {{/each}} </p>
{{/each}}
